{
	// See https://go.microsoft.com/fwlink/?LinkId=733558
	// for the documentation about the tasks.json format
	"version": "2.0.0",
	"command": "gulp",
	"type": "shell",
	"tasks": [
		{
			"label": "default",
			"args": [],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"isBackground": false,
			"problemMatcher": ["$eslint-stylish"],
			"presentation": {
				"reveal": "always",
				"revealProblems": "onProblem"
			}
		}
	]
}
