{
	"parser": "@typescript-eslint/parser",
	"plugins": ["@typescript-eslint", "prettier"],
	"env": {
		"browser": true,
		"es6": true,
		"node": true
	},
	"extends": [
		"plugin:@typescript-eslint/recommended",
		"prettier",
		"prettier/@typescript-eslint"
	],
	"parserOptions": {
		"ecmaVersion": 2018,
		"project": "tsconfig.json",
		"sourceType": "module"
	},
	"rules": {
		"@typescript-eslint/no-non-null-assertion": "off"
	}
}
